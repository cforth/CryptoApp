{
  "fileFromOptionCombobox": {
    "class": "Combobox",
    "var": "fileFromOption",
    "int": {"width": 10},
    "string": {"state": ["readonly"], "values": ["文件", "文件夹", "字符串"]},
    "grid": {"row":0, "column":0, "sticky":["W", "E"]}
  },
  "fileFromButton": {
    "class": "Button",
    "int": {"width": 10},
    "string": {"text": "选择文件"},
    "command": "file_from_button_callback",
    "grid": {"row":0, "column":1, "sticky":["W", "E"]}
  },
  "fileFromPathEntry": {
    "class": "Entry",
    "var": "file_from_path",
    "int": {"width": 40},
    "grid": {"row":0, "column":2, "sticky":["W", "E"]}
  },
  "MD5GenerateButton": {
    "class": "Button",
    "int": {"width": 10},
    "string": {"text": "MD5生成"},
    "command": "md5_generate_button_callback",
    "grid": {"row":0, "column":3, "sticky":["W", "E"]}
  },
  "MD5ResultText": {
    "class": "Text",
    "int": {"width": 60, "height": 20},
    "string": {"wrap": "none"},
    "grid": {"row":1, "column":0, "columnspan":4, "sticky":["W", "E", "N", "S"]}
  },
  "TextScrollbarY": {
    "class": "Scrollbar",
    "string": {"orient": "vertical"},
    "grid": {"row":1, "column":4, "sticky":["N", "S"]}
  },
  "TextScrollbarX": {
    "class": "Scrollbar",
    "string": {"orient": "horizontal"},
    "grid": {"row":2, "column":0, "columnspan":5, "sticky":["W", "E"]}
  }
}